/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = sel, out = selbar);
    And(a = b, b = sel, out = w0);
    And(a = a, b = selbar, out = w1);
    Or(a = w0, b = w1, out = out);
}
